{
  "name": "danfe",
  "displayName": "Danfe",
  "description": "Danfe syntax highlighter",
  "version": "0.0.1",
  
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": "https://github.com/danfe-df/vscode-extension",
  "icon": "assets/df_rounded_more.png",
  "publisher": "Sairash",
  "contributes": {
    "languages": [{
      "id": "danfe",
      "aliases": ["Danfe", "danfe"],
      "extensions": [".df"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./assets/df_file_ext.png",
        "dark": "./assets/df_file_ext.png"
      }
    }],
    "grammars": [{
      "language": "danfe",
      "scopeName": "source.danfe",
      "path": "./syntaxes/danfe.tmLanguage.json"
    }]
  }
}
